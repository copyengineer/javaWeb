  </servlet>
  <servlet-mapping>
    <servlet-name>KaptchaServlet</servlet-name>
    <url-pattern>/code.jpg</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>encoding</display-name>
    <filter-name>encoding</filter-name>
    <filter-class>filter.encoding</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>encoding</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>target_servlet</display-name>
    <servlet-name>target_servlet</servlet-name>
    <servlet-class>servlets.target_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>target_servlet</servlet-name>
    <url-pattern>/target_servlet</url-pattern>
  </servlet-mapping>
  <servlet>
